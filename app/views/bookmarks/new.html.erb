<%= render 'lists/navbar' %>
<div class="container">
  <h1>Add Bookmark</h1>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit %>
  <% end %>
  <%= link_to 'Back', list_path(@list) %>
  <%= link_to 'Home', root_path %>
</div>
